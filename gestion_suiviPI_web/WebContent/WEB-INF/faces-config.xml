<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>classeCtrl</managed-bean-name>
  <managed-bean-class>crtl.ClasseCtrl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>classe</property-name>
   <property-class>com.esprit.gestionPI.persistence.Classe</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pICtrl</managed-bean-name>
  <managed-bean-class>crtl.PICtrl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>pi</property-name>
   <property-class>com.esprit.gestionPI.persistence.PI</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>selectItems</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>selectedItem</property-name>
   <property-class>com.esprit.gestionPI.persistence.PI</property-class>
   <value/>
  </managed-property>
 </managed-bean>
</faces-config>
